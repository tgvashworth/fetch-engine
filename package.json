{
  "name": "fetch-engine",
  "version": "1.0.0",
  "description": "A smart request-making library that makes sure your Javascript client is a good citizen of your distributed system.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf coverage",
    "build": "npm run clean && tsc || true",
    "watch": "npm run clean && tsc --watch",
    "test": "npm run lint && npm run specs-node && npm run specs",
    "lint": "tslint src/**/*.ts",
    "specs": "karma start testing/karma.conf.js",
    "specs-browserstack": "karma start testing/karma.browserstack.conf.js",
    "specs-node": "tape dist/**/*.test.js | tap-min"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuu/fetch-engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phuu/fetch-engine/issues"
  },
  "homepage": "https://github.com/phuu/fetch-engine#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.37.3",
    "ghooks": "^1.0.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-browserstack-launcher": "^0.1.10",
    "karma-coverage": "^0.5.5",
    "karma-electron-launcher": "^0.1.0",
    "karma-tap": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "tap-min": "^1.1.0",
    "tape": "^4.4.0",
    "tsd": "^0.6.5",
    "tslint": "^3.0.0",
    "typescript": "^1.6.2",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
