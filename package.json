{
  "name": "fetch-engine",
  "version": "1.0.0",
  "description": "A smart request-making library that makes sure your Javascript client is a good citizen of your distributed system.",
  "main": "dist/fetch-engine-node.js",
  "browser": "dist/fetch-engine-browser.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc --watch",
    "test": "npm run specs && npm run lint",
    "lint": "find src -name '*.ts' | xargs tslint",
    "specs-node": "tape $(find ./dist -name '*.test.js' | grep -v browser) | tap-min",
    "specs-zuul": "npm run build && zuul -- dist/test-browser.js",
    "specs": "npm run build && npm run specs-node && npm run specs-zuul",
    "zuul": "zuul --local 9998 -- dist/test-browser.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuu/fetch-engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phuu/fetch-engine/issues"
  },
  "homepage": "https://github.com/phuu/fetch-engine#readme",
  "devDependencies": {
    "boom": "^3.1.2",
    "browserify": "^13.0.0",
    "es6-promise": "^3.1.2",
    "ghooks": "^1.0.1",
    "hapi": "^13.3.0",
    "require-globify": "^1.3.0",
    "tap-min": "^1.1.0",
    "tape": "^4.4.0",
    "tape-run": "^2.1.3",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "yargs": "^4.4.0",
    "zuul": "^3.10.1"
  },
  "dependencies": {
    "es6-promise": "^3.1.2"
  }
}
